{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "port": 9229,
      "address": "0.0.0.0",
      "name": "Server",
      "protocol": "inspector",
      "localRoot": "${workspaceFolder}/packages/server",
      "remoteRoot": "/usr/app",
      "restart": true,
      "stopOnEntry": false,
      "sourceMapPathOverrides": {
        "/usr/app/*": "${workspaceRoot}/packages/server/*"
      }
    }
  ]
}
